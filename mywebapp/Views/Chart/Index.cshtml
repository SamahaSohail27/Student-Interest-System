
@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
    @Html.Raw("<script src='https://code.jquery.com/jquery-3.5.1.slim.min.js'></script>")
    @Html.Raw("<script src='https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js'></script>")
    @Html.Raw("<script src='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js'></script>")
    <script src='https://cdn.jsdelivr.net/npm/chart.js'></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-adapter-date-fns@latest/dist/chartjs-adapter-date-fns.bundle.min.js"></script>
</head>
<body>

    <div class="row">
        <!-- Top 5 Interests -->
        <div class="col-md-6">
            <h5 class="mb-3">Top 5 Interests</h5>
            <div class="border p-3" id="topInterestsContainer">

                <!-- Add your interest items here -->
                <div class="interest-item border-bottom mb-2">Interest 1</div>
                <div class="interest-item border-bottom mb-2">Interest 2</div>
                <div class="interest-item border-bottom mb-2">Interest 3</div>
                <div class="interest-item border-bottom mb-2">Interest 4</div>
                <div class="interest-item mb-2">Interest 5</div>
            </div>
        </div>

        <!-- Bottom 5 Interests -->

        <div class="col-md-6">
            <h5 class="mb-3">Bottom 5 Interests</h5>
            <div class="border p-3 id=" id="bottomInterestsContainer">

                <!-- Add your interest items here -->
                <div class="interest-item border-bottom mb-2">Interest 6</div>
                <div class="interest-item border-bottom mb-2">Interest 7</div>
                <div class="interest-item border-bottom mb-2">Interest 8</div>
                <div class="interest-item border-bottom mb-2">Interest 9</div>
                <div class="interest-item mb-2">Interest 10</div>
            </div>
        </div>
    </div>

    <!-- Total Interest Square -->
    <div class="text-center mt-4 border p-3">
        <h4>Total Interests</h4>
        <div class="display-4" id="total_interest">42</div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="chart-container mt-4">
                    <!-- Province Distribution Pie Chart -->
                    <canvas id="provinceChart"></canvas>
                </div>
            </div>
            <div class="col-md-6">
                <div class="chart-container mt-4">
                    <!-- Degree Distribution Pie Chart -->
                    <canvas id="degreeDistributionChart"></canvas>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-6">
                <div class="chart-container mt-4">
                    <!-- Submission Report Graph -->
                    <canvas id="submissionReportChart"></canvas>
                </div>
            </div>
            <div class="col-md-6">
                <div class="chart-container mt-4">
                    <!-- Age Distribution Histogram -->
                    <canvas id="ageDistributionChart"></canvas>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-6">
                <div class="chart-container mt-4">
                    <!-- Department Distribution Pie Chart -->
                    <canvas id="departmentDistributionChart"></canvas>
                </div>
            </div>
            <div class="col-md-6">
                <div class="chart-container mt-4">
                    <!-- Gender Distribution Pie Chart -->
                    <canvas id="genderDistributionChart"></canvas>
                </div>
            </div>
        </div>




        <div class="mt-4">
            <!-- Student Status Table -->
            <h5>Student Status</h5>
            <table class="table" id="studentStatusTable">
                <thead>
                    <tr>
                        <th>Study Status</th>
                        <th>Count</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- Rows will be added here dynamically -->
                </tbody>
            </table>
        </div>

        <div class="row">
            <div class="col-md-6">
                <div class="chart-container mt-4">
                    <!-- Last 30 Days Chart -->
                    <canvas id="last30days"></canvas>
                </div>
            </div>
            <div class="col-md-6">
                <div class="chart-container mt-4">
                    <!-- Last 24 Hours Chart -->
                    <canvas id="last24hrs"></canvas>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-6">
                <div class="table-container mt-4">
                    <h5 id="mostActiveHoursTable">Most Active Hours</h5>
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Hour</th>
                                <th>Count</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>9 AM</td>
                                <td>20</td>
                            </tr>
                            <tr>
                                <td>10 AM</td>
                                <td>15</td>
                            </tr>
                            <!-- Add more rows as needed -->
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-6">
                <div class="table-container mt-4">
                    <h5 id="leastActiveHoursTable">Least Active Hours</h5>
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Hour</th>
                                <th>Count</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>1 PM</td>
                                <td>5</td>
                            </tr>
                            <tr>
                                <td>2 PM</td>
                                <td>8</td>
                            </tr>
                            <!-- Add more rows as needed -->
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-6">
                <div class="table-container mt-4">
                    <h5 id="deadHoursTable">Dead Hours</h5>
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Hour</th>
                                <th>Count</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>3 AM</td>
                                <td>0</td>
                            </tr>
                            <tr>
                                <td>4 AM</td>
                                <td>0</td>
                            </tr>
                            <!-- Add more rows as needed -->
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- HTML code for tables with embedded data -->











</body>
</html>


